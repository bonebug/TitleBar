<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandDrawable" format="reference"/>
        <attr name="collapseDrawable" format="reference"/>
        <attr name="showType" format="enum">
            <enum name="text" value="0"/>
            <enum name="button" value="1"/>
            <enum name="custom" value="2"/>
        </attr>
        <attr name="isTextClickToExtend" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="GomeTextView">
        <attr name="isChangeCharOrNumNewLineStyle" format="boolean"/>
    </declare-styleable>
</resources>